<?xml version="1.0" encoding="utf-8"?>
<!--Portrait-->
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">


    <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="horizontal" android:gravity="fill_horizontal" android:measureWithLargestChild="false"
                android:paddingLeft="0dp" android:paddingTop="0dp" android:paddingBottom="0dp"
                android:paddingRight="0dp">

            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton0" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitEnd" android:background="@color/abs__background_holo_dark" android:baselineAlignBottom="false"
                    android:layout_margin="2dp"/>
            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton1" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitStart" android:background="@color/abs__background_holo_dark" android:baselineAlignBottom="false"
                    android:layout_margin="2dp"/>

        </LinearLayout>
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="horizontal" android:gravity="fill_horizontal" android:measureWithLargestChild="false"
                android:paddingLeft="0dp" android:paddingTop="0dp" android:paddingRight="0dp"
                android:paddingBottom="0dp">

            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton2" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitEnd" android:background="@color/abs__background_holo_dark"
                    android:layout_margin="2dp"/>
            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton3" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitStart" android:background="@color/abs__background_holo_dark"
                    android:layout_margin="2dp"/>

        </LinearLayout>
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="horizontal" android:gravity="fill_horizontal" android:measureWithLargestChild="false"
                android:paddingLeft="0dp" android:paddingTop="0dp" android:paddingRight="0dp"
                android:paddingBottom="0dp">

            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton4" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitEnd" android:background="@color/abs__background_holo_dark"
                    android:layout_margin="2dp"/>
            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton5" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitStart" android:background="@color/abs__background_holo_dark"
                    android:layout_margin="2dp"/>

        </LinearLayout>
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="horizontal" android:gravity="fill_horizontal" android:measureWithLargestChild="false"
                android:paddingLeft="0dp" android:paddingTop="0dp" android:paddingRight="0dp"
                android:paddingBottom="0dp">

            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton6" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitEnd" android:background="@color/abs__background_holo_dark"
                    android:layout_margin="2dp"/>
            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton7" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitStart" android:background="@color/abs__background_holo_dark"
                    android:layout_margin="2dp"/>

        </LinearLayout>
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="horizontal" android:gravity="fill_horizontal" android:measureWithLargestChild="false"
                android:paddingLeft="0dp" android:paddingTop="0dp" android:paddingRight="0dp"
                android:paddingBottom="0dp">

            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton8" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitEnd" android:background="@color/abs__background_holo_dark"
                    android:layout_margin="2dp"/>
            <ImageButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton9" android:cropToPadding="true"
                    android:layout_weight="1" android:adjustViewBounds="true" android:src="@drawable/lightbulb"
                    android:scaleType="fitStart" android:background="@color/abs__background_holo_dark"
                    android:layout_margin="2dp"/>

        </LinearLayout>


    </LinearLayout>
</ScrollView>